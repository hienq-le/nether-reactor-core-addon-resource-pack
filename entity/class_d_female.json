{
    "format_version": "1.10.0",
    "minecraft:client_entity": {
      "description": {
        "identifier": "minecraft:class_d_female",
        "materials": {
          "default": "npc"
        },
        "textures": {
          "classd1": "textures/entity/class_d_f",
          "classd2": "textures/entity/class_d_f2",
          "classd3": "textures/entity/class_d_f3"
        },
        "geometry": {
          "default": "geometry.entity_humanoid_female"
        },
        "animations": {
          "move": "animation.entity_base.moving",
          "attacking": "animation.entity_class_d.attacking",
          "general": "animation.entity_class_d.general",

